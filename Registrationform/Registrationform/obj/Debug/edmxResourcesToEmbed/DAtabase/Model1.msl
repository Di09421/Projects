          <ScalarProperty Name="password" ColumnName="password" />
          <ScalarProperty Name="basic_details_id" ColumnName="basic_details_id" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>